{
  "index_name": "prod_docs",
  "sitemap_urls": ["https://docs.dyte.io/sitemap.xml"],
  "start_urls": [
    {
      "url": "http://docs.dyte.io/",
      "tags": ["docs"]
    },
    {
      "url": "http://docs.dyte.io/react/quickstart/",
      "selectors_key": "react",
      "tags": ["react"]
    },
    {
      "url": "http://docs.dyte.io/react-native/quickstart/",
      "selectors_key": "react-native",
      "tags": ["react-native"]
    },
    {
      "url": "http://docs.dyte.io/javascript/quickstart/",
      "selectors_key": "javascript",
      "tags": ["javascript"]
    },
    {
      "url": "http://docs.dyte.io/flutter/quickstart/",
      "selectors_key": "flutter",
      "tags": ["flutter"]
    },
    {
      "url": "http://docs.dyte.io/android/quickstart/",
      "selectors_key": "android",
      "tags": ["android"]
    },
    {
      "url": "http://docs.dyte.io/ios/quickstart",
      "selectors_key": "ios",
      "tags": ["ios"]
    },
    {
      "url": "http://docs.dyte.io/",
      "tags": ["docs"]
    }
  ],
  "stop_urls": [],
  "selectors": {
    "default": {
      "lvl0": {
        "selector": "(//ul[contains(@class,'menu__list')]//a[contains(@class, 'menu__link menu__link--sublist menu__link--active')]/text() | //nav[contains(@class, 'navbar')]//a[contains(@class, 'navbar__link--active')]/text())[last()]",
        "type": "xpath",
        "default_value": "Documentation"
      },
      "lvl1": "header h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5, article td:first-child",
      "lvl6": "article h6",
      "text": "article p, article li, article td:last-child"
    },
    "react": {
      "lvl0": {
        "selector": ".lv0_link",
        "default_value": "React"
      },
      "lvl1": "header h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5, article td:first-child",
      "lvl6": "article h6",
      "text": "article p, article li, article td:last-child"
    },
    "flutter": {
      "lvl0": {
        "selector": ".lv0_link",
        "default_value": "Flutter"
      },
      "lvl1": "header h1",
      "lvl2": "article h2",
      "lvl3": "article h3",
      "lvl4": "article h4",
      "lvl5": "article h5, article td:first-child",
      "lvl6": "article h6",
      "text": "article p, article li, article td:last-child"
    }
  },
  "strip_chars": " .,;:#",
  "custom_settings": {
    "separatorsToIndex": "_",
    "attributesForFaceting": ["language", "version", "type", "docusaurus_tag"],
    "attributesToRetrieve": [
      "hierarchy",
      "content",
      "anchor",
      "url",
      "url_without_anchor",
      "type"
    ]
  }
}
